@model IEnumerable<InsuranceManagement.Models.Account>

@{
    ViewBag.Title = "Index";
}

<h2>Accounts</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Role.RoleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountName)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.AccountPassWork)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Role.RoleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccountName)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.AccountPassWork)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AccountId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AccountId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AccountId })
        </td>
    </tr>
}

</table>
